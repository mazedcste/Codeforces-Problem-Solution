//http://codeforces.com/contest/200/problem/B
#include <iostream>
#include <cstdio>
using namespace std;
int main ()
{
    int n, x;
    double tot = 0.0;
    scanf (" %d", &n);
    for (int i = 0; i < n; i++) {
        scanf (" %d", &x);
        tot += x;
    }
    tot = tot/n;
    printf ("%lf\n", tot);
    return 0;
}
