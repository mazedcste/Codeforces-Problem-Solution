#include <bits/stdc++.h>
using namespace std;
int main ()
{
    int n;
    char s[15];
    scanf (" %d", &n);
    scanf (" %s", s);
    scanf (" %s", s);
    if (strcmp (s, "week") == 0) {
        if (n == 4) printf ("52");
        else printf ("51");
    }
    if (strcmp (s, "month") == 0) {
        if (n < 29) printf ("12");
        else if (n == 29 || n == 30) printf ("11");
        else if (n == 31) printf ("7");
    }
    return 0;
}
